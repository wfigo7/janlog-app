{
  "name": "@janlog/frontend",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "description": "Janlog Frontend - React Native Expo App",
  "scripts": {
    "start": "npm run start:local",
    "start:local": "cross-env EXPO_PUBLIC_ENV=local expo start",
    "start:dev": "cross-env EXPO_PUBLIC_ENV=development expo start",
    "start:prod": "cross-env EXPO_PUBLIC_ENV=production expo start",
    "dev": "expo start --dev-client",
    "android": "expo start --android",
    "android:local": "cross-env EXPO_PUBLIC_ENV=local expo start --android",
    "android:dev": "cross-env EXPO_PUBLIC_ENV=development expo start --android",
    "ios": "expo start --ios",
    "ios:local": "cross-env EXPO_PUBLIC_ENV=local expo start --ios",
    "ios:dev": "cross-env EXPO_PUBLIC_ENV=development expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build": "expo export",
    "test": "jest",
    "lint": "expo lint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.891.0",
    "@aws-sdk/credential-provider-cognito-identity": "^3.892.0",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-community/datetimepicker": "^8.4.5",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "expo": "~53.0.22",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.5",
    "expo-secure-store": "~14.2.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.13.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-class-static-block": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.4.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "babel-jest": "^30.1.2",
    "cross-env": "^10.0.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "react-test-renderer": "19.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "~5.8.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|@react-navigation|@expo|expo)/)"
    ],
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx|js)",
      "**/*.(test|spec).(ts|tsx|js)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    }
  },
  "private": true
}
